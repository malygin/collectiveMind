$('#render_estimate_post').empty();
$("#render_estimate_post").html('#{escape_javascript(render partial: 'modal_estimate_post')}');
magnificPopupOpen('#popup-estimate');
comments_feed();
comments_expandable_column();
if ($('#gantEditorTemplates').length >0){
ge = new GanttMaster();
ge.init($("#workSpace"));
project = #{@post.tasks_gant.to_json.html_safe};
project.canWrite = false;
ge.loadProject(project);
setTimeout(function() { ge.gantt.refreshGantt()}, 3000);
-#ge.gantt.refreshGantt();
}
history_change("#{estimate_posts_path(@project)}");
comments_collapse_column();
$("[data-toggle=tooltip]").tooltip();
