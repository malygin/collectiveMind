                    %tbody
                      -if @plan_post.post_stages.present?
                        -@plan_post.post_stages.each_with_index do |ps,index|
                          %tr
                            %td
                              =index+1
                            %td
                              -if ps.date_begin and ps.date_end
                                %small.color-green
                                  =Russian::strftime(ps.date_begin, "%d %b %Y")
                                  ="-"
                                  =Russian::strftime(ps.date_end, "%d %b %Y")
