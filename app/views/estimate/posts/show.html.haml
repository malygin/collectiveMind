            %strong Просмотр нововведений

      #bar.progress.progress-small
        .progress-bar
      .tab-content
        #tab1.tab-pane
          .form-horizontal
            %fieldset
              .col-md-6
                %legend.section
                  Описание проекта
                .text-left
                  %h4.color-green Название проекта
                  =@plan_post.name
                %br
                .text-left
                  %h4.color-green Коротко о поекте
                  =@plan_post.goal
                %br
                .text-left
                  %h4.color-green Эссе по проекту. Пояснения и обоснования
                  =@plan_post.content

              .col-md-6
                %legend.section
                  Общая оценка проекта
                  .pull-right
                    -if @est_stat == 1
                      %span.label.label-info Упрощенная
                    -else
                      %span.label.label-success Стандартная
                = simple_format( @post.content)

                -if not  @post.user.jury?
                  Общая оценка проекта
                  %span.color-red(style='font-size: 30px;')="#{ @post.score(@est_stat) } %"
                  %br
                  %span.color-orange
                    #{@post.sum_score} = ( #{@post.second_c} + #{@post.first_c} ) / #{@post.third_c}
                    %br
                    максимальная оценка проекта:
                    %b(style='font-size: 15px;')= @post.max_score
                  %br
                  1) ожидаемая эффективность  первого шага проекта
                  %span.color-red #{ @post.second_c}
                  %br
                  2) ожидаемая эффективность остальных этапов проекта
                  %span.color-red #{ @post.first_c}
                  %br
                  3) ожидаемые неприятности в связи с осуществлением проекта
                  %span.color-red #{ @post.third_c}
                  %br
                  %br
                  %b Общее впечатление от проекта и рекомендации по улучшению
                  = simple_format( @post.content)
                  %br
                  %bОценка проекта в целом:
