                          -if stage.date_begin and stage.date_end
                            %small.color-green
                              =Russian::strftime(stage.date_begin, "%d %b %Y")
                              ="-"
                              =Russian::strftime(stage.date_end, "%d %b %Y")
                            %br
                          =stage.name
                        %td(rowspan="#{rowspan_stage_show(stage)}" class="td_stage_#{stage.id}")
                          =stage.desc
