=link_to '/project/'+@project.id.to_s do
  %span.fa.fa-angle-double-left
  =t('link.back_proc')
%section.widget
  .body
    %h1=t('show.user.club_rating')
    %table.table
      %tbody
        - @users.each_with_index do |user, i|
          = render partial: 'user_rc', locals: {user: user, number: i+1}
    .text-center
      = will_paginate @users
    %br
    =link_to t('link.back_proc'), core_projects_path(@project.id)


